<script type="text/javascript">
	(function(){
	  	var interval;
		function loadFooter()
		{
			if (typeof $ == 'undefined'){
				return;
			}
			if(typeof interval != 'undefined'){
				clearInterval(interval);
			}
			$.ajax({
				url: '/nav/footer.ashx',
				success: function (data) {                
					$('#divFootervX').html(data);
				}
			});
		}
		if (typeof $ != 'undefined'){
			loadFooter();
		}
		else
		{
			interval = setInterval(loadFooter, 1000)
		}
	})();
</script>

<script type="text/javascript">

        function CheckValidation()
        {
            try {
                var isvalid = true;
                for (var i in Page_Validators) {
                    var control = document.getElementById(Page_Validators[i].controltovalidate);
                    if (!Page_Validators[i].isvalid) {
                        isvalid = false;
                        break;
                    } 
                }
                var emailtxtid =ctl00_ctl00_ContentPlaceHolderBase_cphBody_txtEmail;
                if (isvalid) {
                    emailtxtid.classList.remove("highlight");
                }
                else {
                    emailtxtid.classList.add("highlight");                 
                }
                //-- To remove custom validaiton for email
                var emailValidationId =ctl00_ctl00_ContentPlaceHolderBase_cphBody_divEmailValidation;
                if (emailValidationId != undefined)
                    emailValidationId.style.display = "none";   
            } catch (e) { }
        }

    </script>
<script type="text/javascript">
          function KeyPressed(e) {
            var code = (e.keyCode ? e.keyCode : e.which);
              if (code == 13) {       
                document.getElementById('ctl00_ctl00_ContentPlaceHolderBase_cphBody_btnContinue').click();
            }
        }
    </script>

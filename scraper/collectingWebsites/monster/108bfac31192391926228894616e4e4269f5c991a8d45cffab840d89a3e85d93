<script>
    $(function () {
        ///////////////// 
        $(".item-delete").on("click", function () {
            $("#MaskLoader").show();
            var itemID = this.id;
            $.get("/order20/api/cart/deletecartitem/" + itemID, function (data, status) {
                $("#MaskLoader").hide();
                if (status === 'success') {
                    var pLogin = $(".CreateAccountShowMessage").length
                    if (data.data.cartItems.length == 0 && pLogin > 0) {
                        var CurrentDomain = window.location.hostname;
                        CurrentURl = "signin.aspx?redirect=https://" +
                            CurrentDomain +
                            "/default.aspx?HasUserAccount=3";
                        window.location = CurrentURl;
                        return;
                    }
                    else {
                        location.reload();
                    }
                }
            });
        });
    });
</script>

                                args.IsValid=true;
ValidatorValidate(ctl00_ctl00_ContentPlaceHolderBase_cphBody_LiteReg_txtEmailReqValidator, 'LiteRegistrationValidationGroupcustomtxtEmail', null);
                                args.IsValid = ctl00_ctl00_ContentPlaceHolderBase_cphBody_LiteReg_txtEmailReqValidator.isvalid;
if(args.IsValid)
                                {
                                    ValidatorValidate(ctl00_ctl00_ContentPlaceHolderBase_cphBody_LiteReg_txtEmailRegExpValidator, 'LiteRegistrationValidationGroupcustomtxtEmail', null);
                                    args.IsValid = args.IsValid && ctl00_ctl00_ContentPlaceHolderBase_cphBody_LiteReg_txtEmailRegExpValidator.isvalid;
                                }
                            

<script type="text/javascript">

    var LiteRegistration_countryControl = $get("ctl00_ctl00_ContentPlaceHolderBase_cphBody_LiteReg_ddlCountry");
    var LiteRegistration_ZipCodeText = $get("ctl00_ctl00_ContentPlaceHolderBase_cphBody_LiteReg_txtPostalCode");
    var LiteRegistration_stateControl = $get("ctl00_ctl00_ContentPlaceHolderBase_cphBody_LiteReg_ddlState");
    var LiteRegistration_ddlStateReqValidator = "ctl00_ctl00_ContentPlaceHolderBase_cphBody_LiteReg_ddlState" + "ReqValidator";
    var LiteRegistration_txtEmail = $get("ctl00_ctl00_ContentPlaceHolderBase_cphBody_LiteReg_txtEmail");
    var LiteRegistration_txtPhoneNumber = $get("ctl00_ctl00_ContentPlaceHolderBase_cphBody_LiteReg_txtPhoneNumber");
    var LiteRegistration_txtPhoneNumberRegExpValidator = "ctl00_ctl00_ContentPlaceHolderBase_cphBody_LiteReg_txtPhoneNumber" + "RegExpValidator";
    var LiteRegistration_firstName = $get("ctl00_ctl00_ContentPlaceHolderBase_cphBody_LiteReg_txtFirstName");
    var LiteRegistration_lastName = $get("ctl00_ctl00_ContentPlaceHolderBase_cphBody_LiteReg_txtLastName");
    var LiteRegistration_address = $get("ctl00_ctl00_ContentPlaceHolderBase_cphBody_LiteReg_txtAddress");
    var LiteRegistration_city = $get("ctl00_ctl00_ContentPlaceHolderBase_cphBody_LiteReg_txtCity");
    var LiteRegistration_companyName = $get("ctl00_ctl00_ContentPlaceHolderBase_cphBody_LiteReg_txtCompanyName");
    var LiteRegistration_hiringNeed = $get("ctl00_ctl00_ContentPlaceHolderBase_cphBody_LiteReg_ddlHiringNeed");
    var LiteRegistration_companyIndustry = $get("ctl00_ctl00_ContentPlaceHolderBase_cphBody_LiteReg_ddlCompanyIndustry");

    var LiteRegistration_chkMonsterPromoOptin = $get("ctl00_ctl00_ContentPlaceHolderBase_cphBody_LiteReg_chkMonsterPromoOptin");
    var LiteRegistration_chkInformedConsent = $get("ctl00_ctl00_ContentPlaceHolderBase_cphBody_LiteReg_chkInformedConsent");
    var LitRegCheckedList = [];
    function OnAccountCountryChange(combo, eventArgs) {
        if (typeof (combo) != "object")
            return;

        var countryId = CURR_COUNTRY_ID;
        var selectedValue = combo.value;
        if (selectedValue != "" && !isNaN(selectedValue)) {
            countryId = parseInt(selectedValue);
            Services.GeographyLocation.Location.GetStateListByCountryId(countryId, CURR_CHANNEL_ID, onAccountCountrySuccess, onAccountCountryFailure);
            this.Monster.EBiz.Web.Services.MCF.AccountService.GetPhoneValidationSetting(selectedValue, 'hiring', OnPhoneComplete);
        }
        else {
            LiteRegistration_stateControl.parentNode.style.display = "none";
            ValidatorEnable($get(LiteRegistration_ddlStateReqValidator), false);
            ValidatorEnable($get(LiteRegistration_txtPhoneNumberRegExpValidator), false);
        }
    }

    function onAccountCountryFailure(result, eventArgs) {
        publishException;
    }

    function onAccountCountrySuccess(result, eventArgs) {
        var stateList = LiteRegistration_stateControl;
        stateList.options.length = 1;

        for (var i = 1; i < result.length; ++i) {
            stateList.options.add(new Option(result[i].Text, result[i].Value));
        }

        var ddlState = $get(LiteRegistration_ddlStateReqValidator);
        if (stateList.options.length == 1) {
            LiteRegistration_stateControl.parentNode.style.display = "none";
            ValidatorEnable(ddlState, false);
        }
        else {
            LiteRegistration_stateControl.parentNode.style.display = "inline";

            ddlState.enabled = false;
            stateList.selectedIndex = 0;
            ddlState.enabled = true;
        }
    }

    function OnPhoneComplete(result) {

        var PhoneText = LiteRegistration_txtPhoneNumber;
        var PhoneTextREV = $get(LiteRegistration_txtPhoneNumberRegExpValidator);
        var regexp = "";

        PhoneTextREV.isvalid = true;

        if (result != null) {
            regexp = result.RegularExpression;
        }

        if ((regexp == null) || (regexp.length == 0) || (regexp == ".")) {
            PhoneTextREV.validationexpression = "";
            ValidatorEnable(PhoneTextREV, false);
        } else {
            ValidatorEnable(PhoneTextREV, true);
            PhoneTextREV.validationexpression = regexp;
        }
    }

    function OnEmailComplete(sender, args) {
        var rex = /.+\@.+\..+/; // Default regular expression used at the custom control EmailTextBox
        var isValid = rex.test(args.Value);
        if (isValid) {
            var re = /^([\w\-\.]+)@((\[([0-9]{1,3}\.){3}[0-9]{1,3}\])|(([\w\-]+\.)+)([a-zA-Z]{2,4}))$/; // Regular expression to prevent XSS i.e ');alert(123);
            args.IsValid = re.test(args.Value);
            if (!args.IsValid)
                $("#lblEmail").addClass("labelValidationError");
            else
                $("#lblEmail").remove("labelValidationError");
        }

        return args.IsValid;
    }

    

    function CheckBoxTerms() {

        if (LiteRegistration_chkInformedConsent.checked) {
            $('#LitRegElement').find('button').removeAttr("disabled");
            document.getElementById("btnContinue").style.background = "#642891";
        } else {
            $("#LitRegElement").find("button").attr("disabled", "disabled");
            document.getElementById("btnContinue").style.background = "#d8d3dc";
        }

    }
    if (LiteRegistration_chkInformedConsent != null) {
        LiteRegistration_chkInformedConsent.addEventListener("change", CheckBoxTerms);  
    }  
   
</script>

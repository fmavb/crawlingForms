<div class="EditSubscriberConfirmation" id="_ctrl0_ctl84_div1"></div></span><span class="RightPaneDiv12"><div class="ModuleContent mobile-quick-links" id="_ctrl0_ctl87_divModuleContainer">
<div class="ModuleOuterContainer">
<h2 id="_ctrl0_ctl87_lblTitle"><span class="ModuleTitle" id="_ctrl0_ctl87_lblModuleTitle">Most Recent Earnings Materials</span></h2>
<div class="ModuleInnerContainer">
<ul class="items"></ul>
<script src="//widgets.q4app.com/widgets/q4.api.1.11.6.min.js" type="text/javascript"></script>
<script type="text/javascript">
    $('.mobile-quick-links .items').financials({
        reportTypes: ['First Quarter','Second Quarter','Third Quarter','Fourth Quarter'],
        showAllYears: true,
        docCategories: ["Press Release", "Presentation", "Transcript"], 
        template: (
                '{{#quarterPR}}'+
                '<li class="{{docCategory}} col col-1-of-1">'+
                        '<a href="{{docUrl}}" class="btn-arrowAlt white" target="_blank">'+
                            'Earnings Release'+
                        '</a>'+
                '</li>'+
                '{{/quarterPR}}'+
                '{{#quarterTran}}'+
                '<li class="{{docCategory}} col col-1-of-1">'+
                        '<a href="{{docUrl}}" class="btn-arrowAlt white" target="_blank">'+
                            'Earnings Transcript'+
                        '</a>'+
                '</li>'+
                '{{/quarterTran}}'+
                '{{#quarterPres}}'+
                '<li class="{{docCategory}} col col-1-of-1">'+
                        '<a href="{{docUrl}}" class="btn-arrowAlt white" target="_blank">'+
                            'Presentation'+
                        '</a>'+
                '</li>' +
                '{{/quarterPres}}'
        ),

        beforeRender: function(a, data) {

            var quarterPR = [];
            var quarterPres = [];
            var quarterTran = [];

            $.each(data.items, function(i, item) {
                $.each(item.docs, function(i, doc) {
                    // if (!quarterReports.length && (doc.docCategory=="Press Release") || doc.docCategory=="Presentation" || doc.docCategory=="Transcript"){
                    if (!quarterPR.length && doc.docCategory=="Press Release"){
                        quarterPR.push(doc);
                    } else if (!quarterPres.length && doc.docCategory=="Presentation"){
                        quarterPres.push(doc);
                    } else if (!quarterTran.length && doc.docCategory=="Transcript"){
                        quarterTran.push(doc);
                    }  
                });
            });
            data.quarterPR = quarterPR;
            data.quarterPres = quarterPres;
            data.quarterTran = quarterTran;
        }
    });
</script>
</div>
</div>
</div></span></span>
</div>
</main>
</div>

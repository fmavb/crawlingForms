<script>
$('.module-event-latest-past .module_container--content').events({
    dateFormat: {
        date: 'MM/DD/YYYY'
    },
    useMoment: true,
    showPast: true,
    showFuture: true,
    fetchAllYears: true,
    limit: 1,
    tags: ['recent'],
    docCategories: ['Press Release', 'Presentation', 'Transcript', 'PFE Product Revenues', 'Webcast'],
    itemNotFoundMessage: "<div class='col col-1-of-1'>No events available for this year.</div>",
    loadingMessage: '<p class="module_loader"><i class="q4-icon_spinner"></i><span class="sr-only">Loading recent event...</span></p>',
    template: (
        '{{#items}}' +
            '<div class="module_item grid_col grid_col--1-of-3 grid_col--md-1-of-2">' +
                '<div class="module_item-wrap">' +
                    '<div class="module_date-time">' +
                        '<span class="module_date-text">{{date.date}}</span>' +
                    '</div>' +
                    '<div class="module_headline">' +
                        '<a class="module_headline-link" href="{{url}}">{{title}}</a>' +
                    '</div>' +
                    '<div class="module_location">' +
                        '<span class="module_location-text">{{location}}</span>' +
                    '</div>' +
                    

                    '<div class="module_links">' +

                         '{{#financialReports}}{{#docs}}'  +
                             '<div class="module_transcript">' +
                                '<a class="module_link module_webcast-link" href="{{docUrl}}" target="_blank">' +
                                    '<i class="q4-icon_webcast"></i>' +
                                    '<span class="module_link-text">{{docTitle}}</span>' +
                                '</a>' +
                            '</div>' +
                        '{{/docs}}{{/financialReports}}' +
                        '<a class="button" href="/events-and-presentations/default.aspx">All Events</a>' +
                    '</div>' +
                    '<div class="module_presentation_webcast">' +
                       '{{#docs}}' +
                            '<span class="WebcastLink">'+
                                '<span class="{{title}}">' +
                                    '<a href="{{url}}" target="_blank" class="">{{title}} <span class="sr-only">(opens in new window)</span></a>'+
                                '</span>' +
                            '<span>'+
                        '{{/docs}}' +
          '{{#webcast}}' +
                    '<div class="WebcastLink">' +
                     '<span class="Webcast">'+
                        '<a href="{{webcast}}" target="_blank" class="module_link module_webcast-link">Webcast <span class="sr-only">(opens in new window)</span></a></div>' +
                     '</span>' +
                '{{/webcast}}' +
                    '</div>' +
                   '</div>' +
            '</div>' +
        '{{/items}}' 
    ),
    
     itemsComplete: function(e) {
            var webcasts = $(e.target).find('.Webcast > a');
            disclaimer(webcasts, '#disclaimer');
            // $(e.target).find('div[class*="quarterly"] .Release > a').text('Performance Report');
            // $(e.target).find('div[class*="investor"] .PressRelease > a').text('Event Announcement');
            
            var Transcript = $(e.target).find('.Transcript > a');
            disclaimer(Transcript, '#disclaimer1');
        },
    
         complete: function(e) {
            var webcasts = $(e.target).find('.Webcast > a');
            disclaimer(webcasts, '#disclaimer');
            // $(e.target).find('div[class*="quarterly"] .Release > a').text('Performance Report');
            // $(e.target).find('div[class*="investor"] .PressRelease > a').text('Event Announcement');
            
            var Transcript = $(e.target).find('.Transcript > a');
            disclaimer(Transcript, '#disclaimer1');
        },
    beforeRender: function(e, data) {
        console.log(data);
        //tpl.items = tpl.items.shift();
    } 
});

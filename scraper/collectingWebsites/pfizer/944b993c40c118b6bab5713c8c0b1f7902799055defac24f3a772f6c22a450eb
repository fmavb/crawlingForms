<div class="sidebar-quarterly-widget">
<div class="items"></div>
</div>
<a class="btn" href="/financials/quarterly-reports/default.aspx">ALL RESULTS</a>
<script src="https://q4implementation.s3.amazonaws.com/widgets/q4.api.1.11.2.min.js" type="text/javascript"></script>
<script>
$('.sidebar-quarterly-widget .items').financials({
    reportTypes: ['First Quarter','Second Quarter','Third Quarter','Fourth Quarter'],
    loadingMessage: '<span class="loading">Loading...</span>',
    showAllYears: true,
   tags: ['quarter'],
    docCategory: ['Press Release','Presentation','10Q','Transcript','Webcast'],
    template: (
        '{{#quarterReports}}' +
        '<div class="item">'+
            '<div class="heading">'+
                '<span class="quarter">{{shortType}} </span><span class="year">{{year}}</span> EARNINGS'+
            '</div>'+
            '<ul class="docs">'+
                '{{#docs}}'+
                '<li class="{{docCategory}}">'+
                    '<a class="{{docType}}" href="{{docUrl}}" target="_blank">'+
                        // '{{docTitle}}'+
                        '{{docCategory}}'+
                    '</a>'+
                '</li>'+
                '{{/docs}}'+
            '</ul>'+
        '</div>' +
        '{{/quarterReports}}' 
    ),
    beforeRender: function(a, data) {
        // console.log(data);
            var quarterReports = [];
            $.each(data.items, function(i, item) {
                $.each(item.docs, function(i, doc) {
                    if (!quarterReports.length && doc.docCategory=="Press Release"){
                        quarterReports.push(item);
                    }
                     });
            });
            console.log(quarterReports);
            data.quarterReports = quarterReports;

        },
    complete: function(e){
        $('.sidebar-quarterly ul li.10Q a').text('SEC Filings');
        $(".sidebar-quarterly .items .item .docs").prepend($(".sidebar-quarterly .items .item .docs .PFE"));
        $(".sidebar-quarterly .items .item .docs").prepend($(".sidebar-quarterly .items .item .docs .10Q"));
        $(".sidebar-quarterly .items .item .docs").prepend($(".sidebar-quarterly .items .item .docs .Transcript"));
        $(".sidebar-quarterly .items .item .docs").prepend($(".sidebar-quarterly .items .item .docs .Presentation"));
        $(".sidebar-quarterly .items .item .docs").prepend($(".sidebar-quarterly .items .item .docs .Press"));
    }
});
</script>
</div>
</div>

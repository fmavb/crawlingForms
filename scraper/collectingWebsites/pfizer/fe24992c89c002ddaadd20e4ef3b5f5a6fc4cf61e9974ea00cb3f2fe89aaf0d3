<script>
    $('#events-calendar').calendar({
        dateFormat: 'dddd, MMMM DD, YYYY',
        timeFormat:'hh:mm A',
        eventSize: 60,
        eventTemplates: {
            webcast: (
                '{{#webcast}}' +
                    '<div class="WebcastLink">' +
                        '<span class="Webcast">'+
                            '<a href="{{webcast}}" target="_blank" class="ModuleFileLink q4i-icon-webcast">Webcast<span class="sr-only"> (opens in new window)</span></a>' +
                        '</span>'+
                    '</div>' +
                '{{/webcast}}'
            ),
            presentations: (
                '{{#presentations}}'+
                    '<div class="presentation">'+
                        '<div class="PresentationItem">'+
                            '<h4>{{Title}}</h4>'+
                            '<ul>'+
                                '<li class="Presentation">'+
                                    '<a href="{{DocumentPath}}" target="_blank" class="ModuleFile q4i-icon-pdf">'+
                                        '<span class="ModuleDownloadText">Presentation</span>'+
                                        '<span class="FileDetails">'+
                                            '(<span class="FileType">{{DocumentFileType}}</span><span class="FileSize">{{RelatedFileSize}}</span>)'+
                                        '</span>'+
                                        '<span class="sr-only">(opens in new window)</span>'+
                                    '</a>'+
                                '</li>'+
                            '</ul>'+
                        '</div>'+
                    '</div>'+
                '{{/presentations}}'),
            financials: (
                '{{#financials}}'+
                    '<div class="FinancialReport">'+
                        '{{#CoverImagePath}}'+
                            '<div class="CoverImagePane">'+
                                '{{CoverImagePath}}'+
                            '</div>'+
                        '{{/CoverImagePath}}'+
                            '<div class="FinancialContainer">'+
                                '<h4>{{ReportTitle}}</h4>'+
                                '<ul>'+
                                    '{{#Documents}}'+
                                        '<li class="Document {{DocumentCategory}} {{title}}">'+
                                            '<a href="{{DocumentPath}}" class="ReportFile q4i-icon-link" target="_blank">'+
                                                '<span class="ModuleDownloadText">{{DocumentTitle}}</span> '+
                                                '<span class="FileDetails">'+
                                                    '(<span class="FileType">{{DocumentFileType}}</span>'+
                                                     '<span class="FileSize">{{DocumentFileSize}}</span>)'+
                                                '</span>'+
                                            '</a>'+
                                       ' </li>'+
                                    '{{/Documents}}'+
                                '</ul>'+
                            '</div>'+
                    '</div>'+
                '{{/financials}}'),
            event: function(){
                return (
                    '{{#items}}' +
                        '<div class="ModuleEvent active">' +
                            '<div class="close-icon">'+
                                '<span class="q4i-rounded-exit close-popup"></span>'+
                            '</div>'+
                            '<div class="date">{{{date}}} - {{{time}}} {{{timezone}}}</div>' +
                            '<ul class="latest-event">' +
                                '<li>' +
                                    '<a href="{{url}}" class="ModuleHeadlineLink" aria-label="Read full event details">{{title}}</a>' +
                                    // this.location +
                                    // this.speakers +
                                    this.webcast +
                                    this.news +
                                    this.docs +
                                    this.presentations +
                                    this.financials +
                                    this.addToCal +
                                '</li>' +
                            '</ul>' +
                        '</div>' + 
                    '{{/items}}'
                );
            }
        },
        onEventsLoad: function(b, c, d) {
            var e = c[0].items;
            $(d.popupCls).html("");
        },
        calendar: {
            render: function(a) {
                return Mustache.render(
                    '<div class="calendar">'+
                        '<div class="controls">'+
                            '<div>'+
                                '<div class="clndr-previous-button">'+
                                    '<i class="q4i-rounded-leftchevon"></i>'+
                                '</div>'+
                                '<div class="month">{{month}}</div>'+
                                '<div class="clndr-next-button">'+
                                    '<i class="q4i-rounded-rightchevon"></i>'+
                                '</div>'+
                            '</div>'+
                            '<div>'+
                                '<div class="clndr-previous-year-button">'+
                                    '<i class="q4i-rounded-leftchevon"></i>'+
                                '</div>'+
                                '<div class="year">{{year}}</div>'+
                                '<div class="clndr-next-year-button">'+
                                    '<i class="q4i-rounded-rightchevon"></i>'+
                                '</div>'+
                            '</div>'+
                        '</div>'+
                        '<div class="days-container">'+
                            '<div class="days">'+
                                '<div class="headers">'+
                                    '{{#daysOfTheWeek}}'+
                                        '<div class="day-header">{{.}}</div>'+
                                    '{{/daysOfTheWeek}}'+
                                '</div>'+
                                '{{#days}}'+
                                '<div class="{{classes}} {{#events}}{{tag}} {{/events}}"><span>{{day}}</span></div>'+
                                '{{/days}}'+
                            '</div>'+
                        '</div>'+
                    '</div>', 
                a)
            },
            clickEvents: {
                click: function(b) {

                    var c = {
                                date: null,
                                items: []
                            }, 
                        d = $.q4.calendar._proto.options;

                    b.events.length && ($.each(b.events, function(a, b) {
                        var e = b.items;
                        c.items.push({
                            date: moment(e.StartDate).format(d.dateFormat),
                            time: moment(e.StartDate).format(d.timeFormat),
                            docs: e.Attachments,
                            id: new Date(e.StartDate) > new Date && e.EventId,
                            location: e.Location,
                            news: e.EventPressRelease,
                            presentations: e.EventPresentation,
                            speakers: e.EventSpeaker,
                            title: e.Title,
                            url: e.LinkToDetailPage,
                            webcast: e.WebCastLink,
                            financials: e.EventFinancialReport,
                            timezone: e.TimeZone != 0 ? e.TimeZone : ''
                        })
                    }), $(d.popupCls).html(Mustache.render(d.eventTemplates.event(), c)));
                    //disclaimer
                    var webcast = $(d.popupCls).find('.Webcast > a');
                    disclaimer(webcast, '#disclaimer');
                    
                     var Transcript = $(d.popupCls).find('.Document.Transcript > a');
            disclaimer(Transcript, '#disclaimer1');
                    
                    $(d.popupCls).find('.date').text(function(i, text){
                      return text.replace('EST', 'ET');
                    });
                    $('.PFE').hide();
                }
            },
            daysOfTheWeek: ["Sun", "Mon", "Tue", "Wed", "Thur", "Fri", "Sat"],
            ready: function() {
                $('#latest-events').on('click', '.q4-atc-button', function(e){
                    e.preventDefault();
                    $.fancybox.open( $(this).parent().find('.q4-atc-wrapper').clone() );

                    $('.q4-atc-links a').on('mouseenter', function () {
                        var $tooltip = $(this).parents('.q4-atc-inner').find('.q4-atc-tooltip');
                        $tooltip.children('.q4-atc-tooltip-text').html( $(this).children('span').html() );
                        $tooltip.stop(true, true).fadeIn(100);
                    }).on('mouseleave', function () {
                        $(this).parents('.q4-atc-inner').find('.q4-atc-tooltip').stop(true, true).fadeOut(100);
                    });
                });

                 $('#latest-events').on('click', '.close-popup', function(){
                    $(this).closest('.ModuleEvent').removeClass('active');
                });


            }
        }
    });
</script>
</div>

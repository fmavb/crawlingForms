<script>
    $('.event-archive').events({
        dateFormat: {
            date: 'MMMM D, YYYY',
            time: 'hh:mm A'
        },
        useMoment: true,
        showFuture: false,
        fetchAllYears: 0,
        showAllYears: 0,
        tagSelect:'.tags',
        loadingMessage: "<div class='col col-1-of-1'>Loading...</div>",
        itemNotFoundMessage: "<div class='col col-1-of-1'>No events available for this year.</div>",
        tags: [],
        yearSelect: '.ModuleYearSelect',
        yearTemplate: '<option value="{{value}}">{{year}}</option>',
        yearContainer: '.ModuleYearSelect',
        itemContainer: '.ModuleContainerInner',
        itemTemplate: (
            '<div class="ModuleItemRow {{tags}}">' +
                '<div class="ModuleDateContainer col col-1-of-1">' +
                    '<span class="ModuleDate">{{date.date}}</span>' +
                    //'<span class="ModuleTime">{{date.time}} {{timeZone}}</span>' +
                '</div>' +
                '<div class="ModuleContentContainer col col-1-of-1">' +
                    '<div class="headline">'+
                    '<a class="ModuleHeadlineLink" href="{{url}}">{{title}}</a>' +
                    '</div>'+
              
                
                    '{{#pressReleases}}' +
                        '<span class="PressRelease"><a href="{{url}}" class="q4i-icon-html">Press Release</a></span>' +
                    '{{/pressReleases}}' +
                    '{{#presentations}}' +
                        '<span class="presentation"><a href="{{docUrl}}" target="_blank" class="q4i-icon-pdf">Presentation <span class="sr-only">(opens in new window)</span></a></span>' +
                    '{{/presentations}}' +
                    '{{#financialReports}}' +
                        '<span class="FinancialReport">' +
                            '{{#docs}}' +
                                '<span class="{{docCategory}}" >' +
                                    '<a href="{{docUrl}}" target="_blank" class="q4i-icon-link">{{docTitle}} <span class="sr-only">(opens in new window)</span></a>' +
                                '</span>' +
                            '{{/docs}}' +
                        '</span>' +
                    '{{/financialReports}}' +
                    
                    
                    '<span class="ModuleLinks">' +
                        '{{#docs}}' +
                            '<span class="{{type}} {{title}}">' +
                                '<a href="{{url}}" target="_blank" class="q4i-icon-link">{{title}} <span class="sr-only">(opens in new window)</span></a>' +
                            '</span>' +
                        '{{/docs}}' +
                    '</span>' +
                    '{{#webcast}}' +
                        '<span class="WebcastLink">'+
                            '<span class="Webcast">'+
                                '<a href="{{webcast}}" target="_blank" class="q4i-icon-webcast">Webcast <span class="sr-only">(opens in new window)</span></a>'+
                            '</span>' +
                        '</span>'+
                    '{{/webcast}}' +
                    
                '</div>' +
            '</div>'
        ),
        itemsComplete: function(e) {
            var webcasts = $(e.target).find('.Webcast > a');
            disclaimer(webcasts, '#disclaimer');
            $(e.target).find('div[class*="quarterly"] .Release > a').text('Performance Report');
            $(e.target).find('div[class*="investor"] .PressRelease > a').text('Event Announcement');
            
            var Transcript = $(e.target).find('.Transcript > a');
            disclaimer(Transcript, '#disclaimer1');
        },
       complete: function(e){
    $(e.target).find('select').selectbox();

    $(e.target).find('.tags-list').find('>li').first().addClass('active');

    $(e.target).find('.tags-list').on('click','li',function(){
        $(e.target).find('.tags').val($(this).data('tag')).change();
        $(this).siblings().removeClass('active');
        $(this).addClass('active');
    });
}
});
</script>
</div>
</div>
</div></span></span>

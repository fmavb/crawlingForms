<span class="Q4FooterDiv1"><div class="ModuleContent" id="_ctrl0_ctl06_divModuleContainer">
<div class="ModuleOuterContainer">
<div class="ModuleInnerContainer">
<script src="//s21.q4cdn.com/317678438/files/js/jquery-cookie.js" type="text/javascript"></script>
<script type="text/javascript">
    var q4App = $.extend(q4Defaults, {
        secondaryNav: function(){
            $('.navbar-collapse .level1 > li').each(function(){
                if($(this).hasClass('selected')){
                    $(this).addClass('expanded');
                }
            });
            $('.navbar-collapse .level1 li.has-children a').append('<span></span>');
            $('.navbar-collapse').on('click', 'li.has-children a > span', function(e){
                var $this = $(this),
                    $parent = $this.parent();
                    $parent.toggleClass('expanded');
            });
        },
        pluginsInit: function(){
            $('input[type="checkbox"]').iCheck();
        },
        langToggle: function(){
            $('.lang-dropdown').on('click', function(e){
                e.preventDefault();
                $(this).parent().toggleClass('open');
            });
        },
        customAccordion: function(trigger, slide){
            $(trigger).on('click', function(){
                $(this).toggleClass('active');
                $(this).parent().find(slide).slideToggle();
            });
        },
        breadcrumbFunc: function(){
            if(!$('#pageClass').hasClass('homePage-inv')){
                $('.breadCrumb .ModuleInnerContainer').prepend('<a href="/investors-overview/default.aspx" class="breadcrumb-inv-link">INVESTORS</a><span class="BreadcrumbSeparator">/</span>');
            } else {
                $('.breadCrumb .ModuleInnerContainer').prepend('<a class="breadcrumb-inv-link">INVESTORS</a>');
            }
        },
        activateInvestorsHeader: function(){
            $('.header-links').find('li > a:contains("Investors")').parent().addClass('active');
        },
        mobileInvestorsNav: function(){
            var invNav = $('.navbar-collapse').html();
            $('.corporate-navigation').find('.investors').append(invNav);
        },
        addExpandAll: function(faqContainer){
            $(faqContainer).each(function(){

                var faqContainer = $(this);

                faqContainer.find('.ModuleOuterContainer > h2').append('<span class="expandAll">Expand All</span>');

                faqContainer.find('.expandAll').on('click', function(){
                    
                    var question = faqContainer.find('.FaqQuestion');
                    var answer   = faqContainer.find('.FaqQuestion').next('.FaqAnswer');

                    

                    if(!$(this).hasClass('open')){
                        $(this).text('Collapse All');
                        $(this).addClass('open');
                        question.addClass('active');
                        answer.slideDown();
                    } else {
                        $(this).text('Expand All');
                        $(this).removeClass('open');
                        question.removeClass('active');
                        answer.slideUp();
                    }
                });
                
            });
        },
        init: function() {
            var app = this;

            app.cleanUp();

            app.unWrapLink('a.StockPrice');
            app.submitOnEnter('.ModuleSearch');
            app.submitOnEnter('.MailingListUnsubscribeContainer');
            app.validateSubmit('.ModuleSearch');

            app.accessibleNav($('.navbar-collapse'), 'level1');
            app.onMenuToggle();
            app.onMenuExit($('.PaneNavigation .menu-close'));

            $('.PreviewToolBar').previewToolbar();
            $('.MailingListSignupContainer').captcha({
                scrollOnConfirmation: 0,
                submitText: "Subscribe",
            });

            q4App.confirmationScroll($('.PaneFooter .MailingListSignupContainer'), '> .EditSubscriberConfirmation');

            app.secondaryNav();
            app.pluginsInit();
            app.langToggle();
            app.breadcrumbFunc();
            app.activateInvestorsHeader();
            app.mobileInvestorsNav();
        }
    });

    q4App.init();


    function custom_onMenuToggle() {
        $('.LayoutDefaultInner').on('click', '.toggle-mobile', function(e){
            $('.LayoutDefaultInner').toggleClass('mobile-toggled');
            corporateMobileExpand();
            $('.lang-dropdown').parent().removeClass('open');
            if ($('.LayoutDefaultInner').hasClass('mobile-toggled')){
                custom_onMenuClose();
            }
        });
    }

    function custom_onMenuClose() {
        $('.LayoutDefaultInner').one('click', function(e) {
            !$(e.target).closest('.HeaderContainer').length ? $(this).removeClass('mobile-toggled') :custom_onMenuClose();
        });
    }
    
    custom_onMenuToggle();

    $('.corporate-navigation li.has-children > a').after('<span class="q4i-Pfizer_Icons_Plus icon-toggle"></span>');

    function corporateMobileExpand(){

        $('.corporate-navigation').on('click', 'li.has-children > span.icon-toggle', function(e){
            var $this = $(this),
                $parent = $this.parent();
            if (!$parent.hasClass('expanded')) {
                e.preventDefault();
                if($(this).closest('.mega-menu').length){
                    $('.mega-menu').find('.expanded').removeClass('expanded');
                } else {
                    $parent.siblings().removeClass('expanded');
                }
                
                $parent.addClass('expanded');
            } else {
                $parent.removeClass('expanded');
            }
        });
    }





    function disclaimer(el, disclaimer){
        // el
        //  required: true,
        //  type: selector,
        //  desc: Link that has a disclaimer.
        //
        // disclaimer
        //  required: true,
        //  type: selector,
        //  desc: Disclaimer to show when you click on el

    var disclaimerCookie = $.cookie('disclaimerCookie'),
        date = new Date(),
        minutes = 10;
        date.setTime(date.getTime() + (minutes * 144));


    if (disclaimerCookie === undefined) { 
        $(el).on('click', function(e){
            e.preventDefault();

            var url = $(this).attr('href');
            $(disclaimer).find('.accept').attr('href', url);

            $.fancybox.open(disclaimer, {
                padding: [0, 30, 30, 30],
                wrapCSS: 'disclaimer-popUp'
            });
            fancyClose();
        });
        function fancyClose(){
            $('.accept').on('click', function(e) {
                $.fancybox.close();
                   $.cookie("disclaimerCookie", "agree", {
                    path: '/',
                    //expires: date
                   });
             });
        }

    }
}

</script>
</div>
</div>
</div></span><span class="Q4FooterDiv12"><div class="ModuleContent" id="_ctrl0_ctl39_divModuleContainer">
<div class="ModuleOuterContainer">

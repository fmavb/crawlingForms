<script>
         jQuery(document).ready(function() {
              jQuery(".User_Categ").each(function(){
            
              var largest = 0;
            
              jQuery(this).find(".category_header_div").each(function(){
                var findHeight = $(this).height();
                //alert(findHeight);
                if(findHeight > largest){
                  largest = findHeight;
                }  
              })
           // alert(largest);
            var lang="en_US";
            if(lang !='en_US'){
           
              if(lang =='de'){ 
                if($(window).width()>991 && $(window).width()<=1067){
                  largest=largest+40;
                 }
                 if($(window).width()>1067 && $(window).width()<= 1707){
                  largest=largest+16;
                 }
                 if($(window).width()> 1707){
                     largest=largest-9;
                 }
              }
              if(lang =='nl_NL' ){   
                if($(window).width()>991 && $(window).width()<=1009){                 
                   largest=largest+40;
                 }
                 if($(window).width()>1009 && $(window).width()<=1626){                 
                   largest=largest+16;
                 }
                 if($(window).width()>1626){                 
                   largest=largest-9;
                 }
              }
              
              if(lang =='fr' ){   
                if($(window).width()>991 && $(window).width()<= 1386){                 
                   largest=largest+16;
                 }
                 if($(window).width()>1386){
                     largest=largest-9;
                 }
              }
              
              
             }
           
            if($(window).width()>767 && $(window).width()<=991){
                  largest=largest-9;
             } 
              if(lang =='en_US' && $(window).width()>=1153){
                largest=largest-9;
              
              }
              jQuery(this).find(".category_header_div").css({"height":largest+"px"});
              
            });
        });
  
 
  
    jQuery(document).ready(function() {
      jQuery('.nav-toggle').click(function(){
        if($(window).width()<=767){
          //get collapse content selector
          var collapse_content_selector = $(this).attr('data-target');
          // alert(collapse_content_selector);
          //make the collapse content to be shown or hide
          // var toggle_switch = $(this);
          jQuery(collapse_content_selector).toggle('slow',function(){
          }
                                                  );
        };
      }
                                 );
    }
                          );
    jQuery(document).ready(function(){
      jQuery('.listframesdiv').each(function(index){
        var elem =  jQuery(this);
        var url =  elem.attr('src');
        jQuery.ajax({
          url: url,
        }
                   )
          .done(function( data ) {
          console.log(data);
          if (data) {
            var result = jQuery('<div />').append(data).find('#main_div').html();
            console.log('list-----'+ jQuery('<div />').append(data).find('.li_list').html());
            if(jQuery('<div />').append(data).find('.li_list').html() == undefined){
              elem.parent().parent().parent().hide();
            }
            else{
              elem.html(result);
            }
          }
        }
               );
      }
                                   );
    }
                          );
  </script>

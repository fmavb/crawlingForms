<script type="text/javascript">
        jQuery(document).ready(function(){
            jQuery('.search_class').keypress(function(e){
                if(e.which == 13) {
                    e.preventDefault();
                    var searchText = jQuery('.search_class').val();
                    if(searchText == "" || searchText == null || searchText == 'undefined' || searchText.includes("/script")){
                        return true;
                    }
                    <!--*******************************************************************-->
                    var employerFilter = false;
                    searchText = stringreplace(searchText);
                    console.log(searchText);
                    location.href = '/GlassDoorSearch/1/' + employerFilter + '/en_US?q=' + searchText;
                }
            });

            jQuery("#search_btn").bind("click", function(){
                var searchText = jQuery('.search_class').val();
                if(searchText == "" || searchText == null || searchText == 'undefined' || searchText.includes("/script")){
                        return true;
                    }
                <!--*******************************************************************-->
                    var employerFilter = false;
               
                searchText = stringreplace(searchText);
                console.log(searchText);
                location.href = '/GlassDoorSearch/1/' + employerFilter + '/en_US?q=' + searchText;
            });
            
            function stringreplace(searchText){
               
               //added by manish for sanitizing the angular expressions
               var sanitizedstring = searchText.replace(/[/{{}}%`']/g, '');

                //return(searchText);
                return(sanitizedstring); 
            }
            
            
             jQuery(".art_title").autocomplete({
                
                source: function(request, response){
                    var subREGEX = request.term; 
                    var pattern = /^[A-Za-z0-9]+$/;
                    var result = subREGEX.match(pattern);
                    if(result != null && result != '' && result.length > 0){    
                        jQuery.ajax({                               
                            dataType: "json",
                            url: "/GD_AutoSuggestionArticles?q="+request.term+"&employerFilter="+jQuery('.employer_filter').prop('checked')+"&lang=en_US",
                            success: function(data) {
                                response( jQuery.map(data, function(item) {
                                    console.log(item);
                                   console.log("inside0");
                                    if (item != ''){
                                    pageLink = '/article/' + item.UrlName + '/en_US';
                                    
                                        
                                      
                                                                       }
                                    return {
                                        label: (item.Title.length > 60)?jQuery.trim(item.Title).substring(0, 63)+"...":item.Title, 
                                        value: item.Title,
                                        link: pageLink
                                    }                                                                               
                                }));
                            }
                        });
                    }        
                },select: function(event, ui) {
                            location.href=ui.item.link
                   }
                   
            });
        });
    </script>

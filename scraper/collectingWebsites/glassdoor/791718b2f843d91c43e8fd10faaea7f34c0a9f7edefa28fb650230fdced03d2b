<script>
        jQuery(document).ready(function(){
            if(navigator.userAgent.indexOf('GDDroid') !== -1 || navigator.userAgent.indexOf('GDApple') !== -1){            
                jQuery('.MainSiteBrowserHeader').css('display', 'none');
            }
          var a = jQuery(location).attr('href');
            if(a){ 
                var n = a.indexOf("https://help.glassdoor.com/");
                console.log(a);
                if(n>-1){
                    var m=a.lastIndexOf("/");
                    var en = a.indexOf('/en_US/');
                    var de = a.indexOf('/de/');
                    var nl = a.indexOf('/nl_NL/');
                    var fr = a.indexOf('/fr/')
                    var es = a.indexOf('/es/')
                    var pt_BR = a.indexOf('/pt_BR/')
                    var it = a.indexOf('/it/')
                    if((en>-1)||(de>-1)||(nl>-1)||(fr>-1)||(es>-1)||(pt_BR>-1)){
                        if(en>-1){
                            m = m - 6;
                        }
                        if(de>-1){
                            m = m - 3;
                        }
                        if(nl>-1){
                            m = m - 6;
                        }
                        if(fr>-1){
                            m = m - 3;
                        } 
                        if(es>-1){
                            m = m-3;
                        }  
                        if(pt_BR>-1){
                            m = m-6;
                        }
                         if(it>-1){
                            m = m-3;
                        }
                    }
                    var mysubstring = a.substring(26,m);
                    if(mysubstring){
                        var relCanonical = jQuery('<link rel="canonical" href="https://help.glassdoor.com/'+mysubstring+'/en_US"/><link rel="alternate" href="https://hilfe.glassdoor.de/'+mysubstring+'/de" hreflang="de-de" /><link rel="alternate" href="https://help.glassdoor.nl/'+mysubstring+'/nl_NL" hreflang="nl-nl" /><link rel="alternate" href="https://aide.glassdoor.fr/'+mysubstring+'/fr" hreflang="fr-fr" /><link rel="alternate" href="https://ajuda.glassdoor.com.br/'+mysubstring+'/pt_BR" hreflang="pt_BR-pt_BR" /><link rel="alternate" href="https://aiuto.glassdoor.it/GlassdoorHome/it" hreflang="it-it" /><link rel="alternate" href="https://ayuda.glassdoor.com.mx/'+mysubstring+'/es" hreflang="es-es" />');
                        if(relCanonical){
                          jQuery('head').append(relCanonical);
                          console.log('OK'+mysubstring);
                        }
                        
                    }
                }
            }
        })
    </script>

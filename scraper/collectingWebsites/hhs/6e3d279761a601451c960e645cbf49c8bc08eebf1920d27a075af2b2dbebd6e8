<script type="text/javascript">
        $(document).ready(function () {

            $("#programButton").click(function (e) {
                e.preventDefault();
                var pVal = $("#ctl00_pagecontent_ddlPrograms").val();
                var pInd = $("#ctl00_pagecontent_ddlPrograms")[0].selectedIndex;
                if (pInd == 0) {
                    alert("Please select a program.");
                    return;
                }
                location.href = "./document.aspx?rid=3&sid=" + pVal + "&mid=1";
            });
        })
    </script>

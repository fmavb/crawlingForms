<script type="text/javascript">

            function clearFilter() {
                var txt = document.getElementById("ctl00_pagecontent_txtFilter");
                if (txt)
                    txt.value = "";
            }


            function GetClientId(strid) {
                var count = document.forms[0].length;
                var i = 0;
                count = childs.length;

                var eleName;
                for (i = 0; i < count; i++) {
                    /**eleName = document.forms[0].elements[i].id; **/
                    eleName = childs[i].id;
                    alert('found: ' + eleName);
                    pos = eleName.indexOf(strid);
                    if (pos >= 0) {
                        break;
                    }
                }
                alert('result: ' + eleName);
                return eleName;

            }

            function OpenNewWindow(navigateUrl, rating) {

  	           if (rating == 'low')
               {
                    window.open (navigateUrl, '',  'width=680px, height=700px, scrollbars, resizable');
               }
               else
               {
                    window.open (navigateUrl, '',  'width=680px, height=200px, scrollbars, resizable');
               }
               return false;
            }

</script>
<div class="intro" id="breadcrumbs"> <a href="default.aspx" id="ctl00_pagecontent_ucBreadCrumbs_bcHome">Home</a> &gt; <span id="ctl00_pagecontent_ucBreadCrumbs_lblBCafterreport">Study Search</span></div>
<h1 id="content-header"> Study Search</h1>
<div class="page-top-section">
    The review team identified and assessed hundreds of studies released since 1989. Use the search fields below to identify individual studies, or click the “List All” button to generate a complete list of all studies reviewed.

<script type="text/javascript">

                var device_Type = 'desktop';
                var form_Name = '';



                //document.getElementById("PreferedClinic").value = document.getElementById("hidEnquiryPreferedClinic").value;

                document.getElementById("returnback").innerHTML = "Return back to " + "Wirral Health Centre";

                var a = "";

                if (device_Type == "mobile") {
                    a = document.referrer;
                }
                else {
                    a = window.location.href.split(location.search || location.hash || /[?#]/)[0];
                    analyticsClickHandlerForForms("start");
                    if ($(".dialog-content .section-footer").length) {
                        $(".dialog-content .section-footer").hide();
                    }
                }

                var b = document.getElementById("returnback");

                b.setAttribute("href", a);

                document.getElementById("returnbacksorry").innerHTML = "Return back to " + "Wirral Health Centre";

                var c = document.getElementById("returnbacksorry");

                c.setAttribute("href", c);
            </script>

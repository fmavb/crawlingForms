    var MapLink = function (location) {
        var NO_DATA = 1;
        if (location != NO_DATA) {
            if (location === "AZ") { location = "AZB"; }
            var afterOmniture = function (location) { window.location = "Results.aspx?LID=" + location; };
            if (typeof (omni.GetClientOmnitureForHomeNavigation) === 'function') {
                omni.GetClientOmnitureForHomeNavigation(3, location, GetOmnitureImg(), afterOmniture);
            }
        }
    };
